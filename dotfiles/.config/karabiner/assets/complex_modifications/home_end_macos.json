{
  "title": "Home & End for macOS",
  "rules": [
    {
      "description": "Make the Home & End key behave like they would on PCs",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "end"
          },
          "to": {
            "key_code": "right_arrow",
            "modifiers": ["right_command"]
          }
        },
        {
          "type": "basic",
          "from": {
            "key_code": "home"
          },
          "to": {
            "key_code": "left_arrow",
            "modifiers": ["right_command"]
          }
        }
      ]
    }
  ]
}